{
  "name": "typed-jd-union-api",
  "version": "0.0.1",
  "description": "京东开放平台京东联盟NodeJS接口",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "format": "prettier --write src __tests__",
    "test": "jest",
    "build": "rm -rf dist && tsc",
    "gen": "ts-node src/tool/gen.ts && npm run format",
    "doc": "typedoc",
    "buildAndPublish": "npm run format && npm run build && npm publish"
  },
  "author": "logerzhu",
  "repository": {
    "type": "git",
    "url": "git@github.com:logerzhu/typed-jd-union-api.git"
  },
  "bugs": {
    "url": "https://github.com/logerzhu/typed-jd-union-api/issues"
  },
  "homepage": "https://github.com/logerzhu/typed-jd-union-api",
  "license": "MIT",
  "keywords": [
    "TypeScript",
    "Wechat"
  ],
  "dependencies": {
    "axios": "^0.26.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^16.17.1",
    "@types/xml2js": "^0.4.9",
    "jest": "^26.6.3",
    "pre-commit": "^1.2.2",
    "prettier": "2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.23",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.4"
  },
  "pre-commit": [
    "format"
  ],
  "prettier": {
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "options": {
          "singleQuote": true,
          "semi": false,
          "trailingComma": "none"
        }
      }
    ]
  }
}
